{
  "name": "@repo/grpc",
  "module": "./dist/index.mjs",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun run generate:proto && tsup",
    "generate:proto": "bun run generate:proto:auth",
    "generate:proto:auth": "protoc --plugin=../../node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/generated --ts_proto_opt=addGrpcMetadata=true --ts_proto_opt=snakeToCamel=false --proto_path=./src/protos auth.proto",
    "test": "bun test --preload ./tests/setup.ts",
    "test:watch": "bun test --watch --preload ./tests/setup.ts",
    "test:coverage": "bun test --coverage --preload ./tests/setup.ts",
    "test:ci": "bun test --reporter=junit --reporter-outfile=./test-results.xml --preload ./tests/setup.ts",
    "test:update-snapshots": "bun test --update-snapshots --preload ./tests/setup.ts",
    "lint": "eslint src --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "tsup": "^8.5.0",
    "eslint": "^9.28.0",
    "ts-proto": "2.7.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "@grpc/grpc-js": "1.13.4",
    "@grpc/proto-loader": "0.7.15"
  }
}
